        "phpunit/php-invoker": "1.1.*",
        "phpunit/dbunit": "1.2.*",
        "phpunit/phpunit-selenium": "1.3.*",
        "phpunit/phpunit-story": "1.0.*",
        "phpspec/phpspec": "dev-master"
    },
    "scripts": {
        "pre-install-cmd": [],
        "post-install-cmd": []
    },
    "config": {},
    "autoload": {
        "psr-4": {
            "Devtools\\": "vendor/Devtools",
            "Predis\\": "vendor/predis/src"
        }
    }
}
