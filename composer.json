{
    "name": "Devtools",
    "description": "Tools for PHP development",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "predis",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/seagoj/predis.git",
                    "type": "git",
                    "reference": "v1.0"
                }
            }
        }
    ],
    "require": {
        "predis": "dev-v1.0"
    },
    "require-dev": {
